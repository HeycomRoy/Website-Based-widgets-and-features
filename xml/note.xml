<?xml version="1.0" ?>
<!DOCTYPE note [ 
	<!ELEMENT note (to,from,heading,body)>
   <!ELEMENT to (#PCDATA)> 
	<!ELEMENT from (#PCDATA)> 
	<!ELEMENT heading (#PCDATA)> 
	<!ELEMENT body (#PCDATA)> ]> 
<note> 
	<to>Mary</to> 
	<from>John</from>
	<heading>Reminder</heading>
	<body>Meeting on Friday 7pm</body> 
</note> 